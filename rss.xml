  {
    std::vector&amp;lt;int&amp;gt; v = {3, 1, 4};
    v.&lt;span style=&#34;color:#ff0000&#34;&gt;resize&lt;/span&gt;(5);

    std::for_each(v.begin(), v.end(), [](int x) { std::cout &amp;lt;&amp;lt; x &amp;lt;&amp;lt; std::endl; });
  }
  std::cout &amp;lt;&amp;lt; std::endl;

  // 減少
  {
    std::vector&amp;lt;int&amp;gt; v = {3, 1, 4};
    v.&lt;span style=&#34;color:#ff0000&#34;&gt;resize&lt;/span&gt;(1);

    std::for_each(v.begin(), v.end(), [](int x) { std::cout &amp;lt;&amp;lt; x &amp;lt;&amp;lt; std::endl; });
  }
}
&lt;/code&gt;&lt;/pre&gt;&lt;/p&gt;
